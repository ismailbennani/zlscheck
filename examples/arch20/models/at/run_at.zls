open MyOp

node main () =
    let rec t = make 0. fby (t + (make 0.01)) in
    let throttle = make 100. in
    let brake = make 0. in

    let rpm, gear, speed = Autotransd.autotrans (t, (throttle ,brake)) in

    print_endline ("t = " ^ (string_of_float (get t)) ^
                 "\nspeed = " ^ (string_of_float (get speed)));
    flush stdout
