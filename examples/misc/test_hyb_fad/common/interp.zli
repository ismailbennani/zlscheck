val interp1 :
    FadFloat.t Array.t (* x *)
  * FadFloat.t Array.t (* y *)
  -A->
    (FadFloat.t -A-> FadFloat.t) (* lookup function (linear interpolation and extrapolation) *)

val interp2 :
    FadFloat.t Array.t       (* x *)
  * FadFloat.t Array.t       (* y *)
  * FadFloat.t Array.t Array.t (* z *)
  -A->
    (FadFloat.t * FadFloat.t -A-> FadFloat.t) (* lookup function (linear interpolation and extrapolation)*)

